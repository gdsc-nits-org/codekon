{
  "name": "codekon",
  "version": "0.0.1",
  "description": "Entirety of GDSC NITS project templates at your fingertips",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev:node": "tsc && node --enable-source-maps dist/src/index.js",
    "dev": "pnpm run \"/dev:/\"",
    "lint": "tsc --noEmit && eslint --cache src tests --fix",
    "format": "prettier --write --cache src tests"
  },
  "keywords": [],
  "repository": "https://github.com/gdsc-nits-org/codekon",
  "homepage": "https://github.com/gdsc-nits-org/codekon#readme",
  "author": "GDSC NITS <gdsc@nits.ac.in>",
  "contributors": [
    "Adhiraj Dutta <bytehead.dev@gmail.com>"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "commander": "^12.1.0",
    "execa": "^9.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/node": "^20.14.2",
    "@types/prompts": "^2.4.9",
    "eslint": "~9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "jest": "^29.7.0",
    "kolorist": "^1.8.0",
    "prettier": "3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
